    <!-- Examples -->
    <script src="~/Assests/Dashboard/assets/javascripts/ui-elements/examples.notifications.js"></script>


    <!--toaster Noticiation method-->
    <script>
        function SendNoti(title, text, type) {
            new PNotify({
                title: title,
                text: text,
                type: type
            });
        }
    </script>
