    <style>

        .no-js #loader {
            display: none;
        }

        .js #loader {
            display: block;
            position: center;
            left: 100px;
            top: 0;
        }

        .se-pre-con {
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 999;
            background: url('/Assests/Login/images/loaderFinal.gif') center no-repeat #fff;
        }

        .se-pre-con1 {
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 999;
            background: url('../Login/images/throbber_12.gif') center no-repeat #fff;
        }
    </style>
    <script>
        //paste this code under the head tag or in a separate js file.
        // Wait for window load
        $(window).load(function () {
            // Animate loader off screen
            $(".se-pre-con").fadeOut("slow");
        });
    </script>

</head>
<body>
    <div class="se-pre-con"></div>
    <section class="body-sign">
        @RenderBody()
    </section>
    <script src="~/Assests/Dashboard/assets/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="~/Assests/Dashboard/assets/vendor/bootstrap/js/bootstrap.js"></script>
